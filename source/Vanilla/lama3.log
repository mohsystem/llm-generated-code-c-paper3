void removeSmallest(int* arr, int n, int** result, int* resultSize) {
    if (n == 0) {
        *result = NULL;
        *resultSize = 0;
        return;
    }

    int minIndex = 0;
    for (int i = 1; i < n; i++) {
        if (arr[i] < arr[minIndex]) {
            minIndex = i;
        }
    }

    *resultSize = n - 1;
    *result = (int*)malloc((*resultSize) * sizeof(int));
    for (int i = 0; i < minIndex; i++) {
        (*result)[i] = arr[i];
    }
    for (int i = minIndex + 1; i < n; i++) {
        (*result)[i - 1] = arr[i];
    }
}

