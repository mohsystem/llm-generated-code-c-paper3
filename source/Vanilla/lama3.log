            if (clientSocket < 0) {
                std::cerr << "Error accepting client\n";
                continue;
            }
            std::cout << "Client connected\n";
            std::thread clientThread(&ChatServer::handleClient, this, clientSocket);
            clientThread.detach();
        }
    }
