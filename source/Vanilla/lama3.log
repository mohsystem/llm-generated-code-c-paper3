public class Task14 {
    public static int findMiddleIndex(int[] arr) {
        if (arr.length == 0) return -1;
        int leftSum = 0;
        int rightSum = 0;
        for (int num : arr) rightSum += num;
        for (int i = 0; i < arr.length; i++) {
            rightSum -= arr[i];
            if (leftSum == rightSum) return i;
            leftSum += arr[i];
        }
        return -1;
