            if (num_set.find(num - 1) == num_set.end()) {
                int current_num = num;
                int current_streak = 1;
                while (num_set.find(current_num + 1) != num_set.end()) {
                    current_num += 1;
                    current_streak += 1;
                }
                longest_streak = std::max(longest_streak, current_streak);
            }
        }
        return longest_streak;
    }
};

