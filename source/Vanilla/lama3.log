#include <iostream>
#include <sstream>
#include <string>

std::string toJadenCase(const std::string& phrase) {
    if (phrase.empty()) {
        return "";
    }
    std::istringstream iss(phrase);
    std::string word;
    std::string result;
    while (iss >> word) {
        word[0] = toupper(word[0]);
        for (size_t i = 1; i < word.length(); ++i) {
            word[i] = tolower(word[i]);
        }
        result += word + " ";
    }
    // Remove the trailing space
    if (!result.empty()) {
        result.pop_back();
    }
    return result;
}

