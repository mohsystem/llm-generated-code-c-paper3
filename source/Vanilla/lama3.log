        long maxProduct = 0;
        for (int i = 0; i <= digits.length() - span; i++) {
            long product = 1;
            for (int j = i; j < i + span; j++) {
                product *= digits.charAt(j) - '0';
            }
            maxProduct = Math.max(maxProduct, product);
        }
        return maxProduct;
    }
