bool isValidFileName(const char* fileName) {
    regex_t regex;
    const char* pattern = "^.+\\.(java|py|c|cpp|cs|js|html|css)$";
    if (regcomp(&regex, pattern, REG_EXTENDED) != 0) {
        return false;
    }
    int status = regexec(&regex, fileName, 0, NULL, 0);
    regfree(&regex);
    return status == 0;
}

