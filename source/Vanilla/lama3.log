    for (int i = 0; i < sessionCount; i++) {
        if (strcmp(sessions[i].username, username) == 0) {
            sessions[i].isActive = 0;
            printf("Session terminated: %s\n", username);
            return;
        }
    }
    printf("Session does not exist: %s\n", username);
}

int main() {
    createSession("user1");
    terminateSession("user1");
    return 0;
