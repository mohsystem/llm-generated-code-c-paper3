public class Task20 {
    public static int findK(int n, int p) {
        String str = Integer.toString(n);
        long sum = 0;
        for (int i = 0; i < str.length(); i++) {
            sum += (long) Math.pow(Character.getNumericValue(str.charAt(i)), p + i);
        }
        if (sum % n == 0) {
