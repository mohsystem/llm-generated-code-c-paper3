int shortestSubarray(int* nums, int n, int k) {
    int prefixSum[n + 1];
    prefixSum[0] = 0;
    for (int i = 0; i < n; i++) {
        prefixSum[i + 1] = prefixSum[i] + nums[i];
    }
    int ans = INT_MAX;
    for (int i = 0; i < n; i++) {
        for (int j = i; j < n; j++) {
            int sum = prefixSum[j + 1] - prefixSum[i];
            if (sum >= k) {
                ans = (ans < (j - i + 1)) ? ans : (j - i + 1);
            }
        }
    }
    return ans == INT_MAX ? -1 : ans;
}

